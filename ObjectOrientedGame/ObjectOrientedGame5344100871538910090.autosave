// Space shooter

Crosshair crosshair;
Background[] background = new Background[2];



void setup() {
  size(800, 800);
  noCursor();
  frameRate(60);

  crosshair = new Crosshair();
}

void draw() {


  background(0);

for (int  l = 0; l < background.length; l +=1) { 
  background[0] = new Background(100, 255, 0, 0);
}
  crosshair.Display();

  for (int i = 0; i < background.length; i += 1) {
    background[i].Display();
  }
}
